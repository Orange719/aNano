#!/bin/bash

#Colors
Y='\033[1;33m'; #Yellow
G='\033[1;30m'; #Light Gray
W='\033[1;37m' #White
B='\033[1;34m' #Light Blue
#Print Nano
#now you can print 69 nanos! how epic is that??
for i in $( seq 1 69 ); do
  printf "
                            ${Y}...
                             ....
                             ...
                        ${G}((((((((((((
                      ((((((${W}.${G}((((((((
                     (((( (${W}..${G}(((${W}.${G}(((((
                      ((((${W}....... .${G}((((
                     (((${W}. . .......${G}(((
                     (((${W}........  .,${G}((
                    (((((${W}........... ${G}(
                    (((((${W}.........${G}((((
                      ${B}..,**,,,,***. ${G}((
                    ${B}...............,
                    .....,... .. .......
                 ..,....... .. ,.......
                ,...,...... .. .........
                ........................
               .......,..................
              ............................\n"; done
              
